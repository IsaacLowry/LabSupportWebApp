
@{
    ViewBag.Title = "QueueList";
}

<style>

    .openClasses {
      border-collapse: collapse;
      width: 100%;
    }

        .openClasses th {
            background-color: #000b59;
            border: 4px solid white;
            color: white;
            text-align: center;
            border-radius: 10px;
        }

        .openClasses td, th {
            border: 2px dashed #a7e6fc;
            text-align: left;
            padding: 8px;
        }

        .openClasses tr:nth-child(even) {
            background-color: #a7e6fc;
        }
</style>

<div class="mainContainer">
    <div class="row" style="margin-top: 10vh; font-size: 1.5rem;">
        <div class="col-lg-4 col-lg-offset-4">
            <h2 class="teachTitle">Open Practical Sessions</h2>
            <div style="margin-top: 5vh;">
                <table class="openClasses">
                    <tr>
                        <th>Session Name</th>
                        <th>Students In Queue</th>
                        <th>Elapsed Time</th>
                        <th>Join Session</th>
                    </tr>
                    <tr>
                        <td>Human Computer Interaction Lab</td>
                        <td>34</td>
                        <td>20 minutes</td>
                        <td style="text-align:center"><a href="@Url.Action("HeadOfQueue", "home")"><i class="fas fa-external-link-alt"></i></a></td>
                    </tr>
                    <tr>
                        <td>Human Computer Interaction Lab</td>
                        <td>34</td>
                        <td>20 minutes</td>
                        <td style="text-align:center"><a href="@Url.Action("HeadOfQueue", "home")"><i class="fas fa-external-link-alt"></i></a></td>
                    </tr>
                </table>
            </div>
        </div>

    </div>
</div>

    
